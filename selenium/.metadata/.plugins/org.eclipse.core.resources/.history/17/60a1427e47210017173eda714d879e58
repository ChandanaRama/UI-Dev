package JavaPgms;

public class FizzBuzzPgm {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[]num = {10,3,5,15};
		for(int i = 0;i<num.length;i++){
			if(num[i]%3 == 0){
				System.out.println("Fizz"+""+num[i]);
			}
			else if (num[i]%5 == 0){
				System.out.println("Buzz"+""+num[i]);
			}
			else if (num[i]%3 == 0 && num[i]%5 == 0){
				System.out.println("Fizz and Buzz"+""+num[i]);
			}
		}
		

	}

}
